        "title": "API de Músicas Tião Carreiro e Pardinho",
        "description": "API para gerenciamento de músicas e sugestões",
        "contact": {
            "email": "tulioalves17@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api",
